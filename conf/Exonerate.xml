<?xml version="1.0" encoding="ISO-8859-1"?>
<triannotConf triannot_version="">
	<section name="Exonerate">

		<entry key="configurationDependencies" description="A list of paths to other configuration entries that must be defined to allow the use of this tool">
			<entry>PATHS|soft|Exonerate|bin</entry>
		</entry>

		<entry key="commonParameters">
			<entry key="database">
				<entry key="mandatory">yes</entry>
				<entry key="type">configEntry</entry>
				<entry key="listOfValuesPath">PATHS|db</entry>
				<entry key="listOfValuesMode">keys</entry>
				<entry key="listOfValuesFilter">
					<entry key="filterPath">availableFormats</entry>
					<entry key="filterMode">values</entry>
					<entry key="filterAllowedValues">
						<entry>NucleicBlast</entry>
						<entry>ProteicBlast</entry>
					</entry>
				</entry>
			</entry>
			<entry key="queryType" description="Possible values : protein|dna">
				<entry key="mandatory">yes</entry>
				<entry key="type">string</entry>
				<entry key="possibleValues">
					<entry>dna</entry>
					<entry>protein</entry>
				</entry>
			</entry>
		</entry>

		<entry key="execParameters">
			<entry key="targetType" description="Possible values : protein|dna">
				<entry key="defaultValue">dna</entry>
				<entry key="type">string</entry>
				<entry key="possibleValues">
					<entry>dna</entry>
					<entry>protein</entry>
				</entry>
			</entry>
			<entry key="exhaustive" description="Specify whether or not exhaustive alignment should be used. If true : very slow!. Possible values: yes|no">
				<entry key="defaultValue">no</entry>
				<entry key="type">boolean</entry>
				<entry key="trueValue">yes</entry>
				<entry key="falseValue">no</entry>
			</entry>
			<entry key="model" description="Specify the alignment model to use. Possible values: ungapped|ungapped:trans|affine:global|affine:bestfit|affine:local|affine:overlap|est2genome|ner|protein2dna|protein2dna:bestfit|protein2genome|protein2genome:bestfit|coding2coding|coding2genome|cdna2genome|genome2genome">
				<entry key="defaultValue">est2genome</entry>
				<entry key="type">string</entry>
				<entry key="possibleValues">
					<entry>ungapped</entry>
					<entry>ungapped:trans</entry>
					<entry>affine:global</entry>
					<entry>affine:bestfit</entry>
					<entry>affine:local</entry>
					<entry>affine:overlap</entry>
					<entry>est2genome</entry>
					<entry>ner</entry>
					<entry>protein2dna</entry>
					<entry>protein2dna:bestfit</entry>
					<entry>protein2genome</entry>
					<entry>protein2genome:bestfit</entry>
					<entry>coding2coding</entry>
					<entry>coding2genome</entry>
					<entry>cdna2genome</entry>
					<entry>genome2genome</entry>
				</entry>
			</entry>
			<entry key="score" description="This is the overall score threshold">
				<entry key="defaultValue">250</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
			</entry>
			<entry key="percent" description="Report only alignments scoring at least this percentage of the maximal score for each query">
				<entry key="defaultValue">50</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
				<entry key="maxValue">100</entry>
			</entry>
			<entry key="bestn" description="Report the best N results for each query">
				<entry key="defaultValue">5</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
			</entry>
			<entry key="maxIntron" description="Maximum intron length limit">
				<entry key="defaultValue">20000</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
			</entry>
			<entry key="minIntron" description="Minimum intron length limit">
				<entry key="defaultValue">1</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
			</entry>
			<entry key="softMaskTarget" description="Specify that the target is lower case masked. Possible values: yes|no|true|false">
				<entry key="defaultValue">no</entry>
				<entry key="type">boolean</entry>
				<entry key="trueValue">yes</entry>
				<entry key="falseValue">no</entry>
			</entry>
			<entry key="refine" description="Specify to refine alignments generated by heuristic methods. full is very slow. Possible values: none|region|full">
				<entry key="defaultValue">region</entry>
				<entry key="type">string</entry>
				<entry key="possibleValues">
					<entry>none</entry>
					<entry>full</entry>
					<entry>region</entry>
				</entry>
			</entry>
			<entry key="miniBankPrefix" description="When using a minibank please provide a miniBankPrefix">
				<entry key="type">string</entry>
			</entry>
			<entry key="outFile" description="Name to use for the Exonerate out file">
				<entry key="defaultValue">{step}_EXONERATE_{database}.res</entry>
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
			</entry>
		</entry>

		<entry key="parserParameters">
			<entry key="coverageThreshold">
				<entry key="defaultValue">0.80</entry>
				<entry key="type">number</entry>
				<entry key="minValue">0</entry>
				<entry key="maxValue">1</entry>
			</entry>

			<!-- Custom feature source -->
			<entry key="sourceTag" description="Source Tag to use during features creation">
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
				<entry key="defaultValue">EXONERATE_{database}</entry>
			</entry>

			<!-- Output file names -->
			<entry key="gffFile" description="GFF File name">
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
				<entry key="defaultValue">{step}_EXONERATE_{database}.gff</entry>
			</entry>

			<entry key="emblFile" description="EMBL File name">
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
				<entry key="defaultValue">{step}_EXONERATE_{database}.embl</entry>
			</entry>

			<!-- EMBL file creation constraints -->
			<entry key="EMBLFormat">
				<entry key="type">configEntry</entry>
				<entry key="listOfValuesPath">EMBL|formats|match</entry>
				<entry key="listOfValuesMode">keys</entry>
				<entry key="defaultValue">joinedMatchPartOnly</entry>
			</entry>

			<entry key="EMBLRename.match">
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
			</entry>
			<entry key="EMBLRename.match_part">
				<entry key="type">string</entry>
				<entry key="needSubstitution">yes</entry>
				<entry key="defaultValue">EXO_{database}</entry>
			</entry>
		</entry>

		<entry key="reconstructionParameters">
			<entry key="activateReconstruction" description="Activate or desactivate the global result files reconstruction for splitted sequences">
				<entry key="defaultValue">yes</entry>
				<entry key="type">boolean</entry>
				<entry key="trueValue">yes</entry>
				<entry key="falseValue">no</entry>
			</entry>
		</entry>

	</section>
</triannotConf>
